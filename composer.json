{
  "name": "azuyalabs/yasumi",
  "description": "Yasumi is an easy PHP Library for calculating national holidays.",
  "keywords": [
    "holiday",
    "calendar",
    "calculation",
    "celebration",
    "php",
    "date",
    "time"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Sacha Telgenhof",
      "email": "me@sachatelgenhof.nl"
    }
  ],
  "require": {
    "php": ">=5.5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.6",
    "fzaninotto/faker": "~1.4",
    "mikey179/vfsStream": "~1.5.0",
    "fabpot/php-cs-fixer": "^1.11"
  },
  "autoload": {
    "psr-4": {
      "Yasumi\\": "src/Yasumi/"
    }
  },
  "autoload-dev": {
    "psr-4": {"Yasumi\\tests\\": "tests/"}
  },
  "scripts": {
    "php-cs-fixer": "./vendor/bin/php-cs-fixer fix ."
  }
}
